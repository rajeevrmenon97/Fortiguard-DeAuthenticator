{
  "manifest_version": 2,

  "name": "NIT Calicut Logoutter",
  "description": "This extension automatically logs you out of NITC network",
  "version": "1.0",
  "icons": {   		"16": "icon16.png",
           		"48": "icon48.png",
          		"128": "icon128.png"},
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"storage"
  ]
}
